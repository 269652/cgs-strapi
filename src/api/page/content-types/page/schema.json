{
  "displayName": "Page",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "header": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::header.header"
    },
    "footer": {
      "type": "component",
      "repeatable": false,
      "component": "page.footer"
    },
    "pageContent": {
      "type": "dynamiczone",
      "components": ["page.row", "page.group"]
    }
  }
}
